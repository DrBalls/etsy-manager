{
  "name": "@etsy-manager/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared types, utilities, and API clients for Etsy Store Manager",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.mjs",
      "require": "./dist/api.js"
    }
  },
  "scripts": {
    "build": "tsup || node build-windows.js",
    "build:windows": "node build-windows.js",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "zod": "^3.23.8",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "ioredis": "^5.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist"
  ]
}
